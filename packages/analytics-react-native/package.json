{
  "name": "@amplitude/analytics-react-native",
  "version": "0.0.0",
  "description": "Official React Native SDK",
  "keywords": [
    "analytics",
    "amplitude",
    "react-native",
    "ios",
    "android"
  ],
  "author": "Amplitude Inc",
  "homepage": "https://github.com/amplitude/Amplitude-TypeScript",
  "license": "MIT",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amplitude/Amplitude-TypeScript.git"
  },
  "scripts": {
    "build": "yarn version-file && bob build",
    "fix": "yarn fix:eslint & yarn fix:prettier",
    "fix:eslint": "eslint '{src,test}/**/*.ts' --fix",
    "fix:prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "yarn lint:eslint & yarn lint:prettier",
    "lint:eslint": "eslint '{src,test}/**/*.ts'",
    "lint:prettier": "prettier --check \"{src,test}/**/*.ts\"",
    "test": "yarn test:web && yarn test:mobile",
    "test:web": "jest --setupFiles './test/mock/setup-web.ts'",
    "test:mobile": "jest --setupFiles './test/mock/setup-mobile.ts'",
    "typecheck": "tsc -p ./tsconfig.json",
    "version": "yarn build",
    "version-file": "node -p \"'export const VERSION = \\'' + require('./package.json').version + '\\';'\" > src/version.ts",
    "typescript": "tsc --noEmit",
    "prepare": "yarn version-file && bob build",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet"
  },
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "amplitude-react-native.podspec",
    "!android/build",
    "!ios/build",
    "!**/test"
  ],
  "bugs": {
    "url": "https://github.com/amplitude/Amplitude-TypeScript/issues"
  },
  "dependencies": {
    "@amplitude/analytics-connector": "1.4.4",
    "@amplitude/analytics-core": "^0.6.1",
    "@amplitude/analytics-types": "^0.5.1",
    "@amplitude/ua-parser-js": "^0.7.31",
    "tslib": "^2.3.1",
    "@react-native-async-storage/async-storage": "^1.17.7"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.17.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
